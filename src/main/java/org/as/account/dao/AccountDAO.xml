<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.as.account.dao.AccountDAO">
    <insert id="addAccount">
        INSERT INTO TB_ACCOUNT (USER_ID, USER_NAME, PWD ) VALUE (#{userId}, #{userName}, #{pwd})
    </insert>
</mapper>